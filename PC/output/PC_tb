#! /usr/bin/vvp
:ivl_version "12.0 (stable)" "(v12_0-dirty)";
:ivl_delay_selection "TYPICAL";
:vpi_time_precision + 0;
:vpi_module "/usr/lib/ivl/system.vpi";
:vpi_module "/usr/lib/ivl/vhdl_sys.vpi";
:vpi_module "/usr/lib/ivl/vhdl_textio.vpi";
:vpi_module "/usr/lib/ivl/v2005_math.vpi";
:vpi_module "/usr/lib/ivl/va_math.vpi";
S_0x5e98ab5471a0 .scope module, "PC_tb" "PC_tb" 2 1;
 .timescale 0 0;
v0x5e98ab591ff0_0 .var "clk", 0 0;
v0x5e98ab5920c0_0 .net "pc_out", 7 0, v0x5e98ab591df0_0;  1 drivers
v0x5e98ab592190_0 .var "reset", 0 0;
E_0x5e98ab570d70 .event posedge, v0x5e98ab5706c0_0;
S_0x5e98ab547350 .scope module, "pc_inst" "PC" 2 12, 3 1 0, S_0x5e98ab5471a0;
 .timescale 0 0;
    .port_info 0 /INPUT 1 "clk";
    .port_info 1 /INPUT 1 "reset";
    .port_info 2 /OUTPUT 8 "pc";
v0x5e98ab5706c0_0 .net "clk", 0 0, v0x5e98ab591ff0_0;  1 drivers
v0x5e98ab591df0_0 .var "pc", 7 0;
v0x5e98ab591ed0_0 .net "reset", 0 0, v0x5e98ab592190_0;  1 drivers
E_0x5e98ab571410 .event posedge, v0x5e98ab591ed0_0, v0x5e98ab5706c0_0;
    .scope S_0x5e98ab547350;
T_0 ;
    %wait E_0x5e98ab571410;
    %load/vec4 v0x5e98ab591ed0_0;
    %flag_set/vec4 8;
    %jmp/0xz  T_0.0, 8;
    %pushi/vec4 0, 0, 8;
    %assign/vec4 v0x5e98ab591df0_0, 0;
    %jmp T_0.1;
T_0.0 ;
    %load/vec4 v0x5e98ab5706c0_0;
    %flag_set/vec4 8;
    %jmp/0xz  T_0.2, 8;
    %load/vec4 v0x5e98ab591df0_0;
    %addi 1, 0, 8;
    %assign/vec4 v0x5e98ab591df0_0, 0;
T_0.2 ;
T_0.1 ;
    %jmp T_0;
    .thread T_0;
    .scope S_0x5e98ab5471a0;
T_1 ;
    %delay 5, 0;
    %load/vec4 v0x5e98ab591ff0_0;
    %inv;
    %store/vec4 v0x5e98ab591ff0_0, 0, 1;
    %jmp T_1;
    .thread T_1;
    .scope S_0x5e98ab5471a0;
T_2 ;
    %pushi/vec4 0, 0, 1;
    %store/vec4 v0x5e98ab591ff0_0, 0, 1;
    %pushi/vec4 1, 0, 1;
    %store/vec4 v0x5e98ab592190_0, 0, 1;
    %delay 10, 0;
    %pushi/vec4 0, 0, 1;
    %store/vec4 v0x5e98ab592190_0, 0, 1;
    %end;
    .thread T_2;
    .scope S_0x5e98ab5471a0;
T_3 ;
    %wait E_0x5e98ab570d70;
    %vpi_call 2 27 "$display", "PC = %h", v0x5e98ab5920c0_0 {0 0 0};
    %jmp T_3;
    .thread T_3;
    .scope S_0x5e98ab5471a0;
T_4 ;
    %delay 100, 0;
    %vpi_call 2 32 "$finish" {0 0 0};
    %end;
    .thread T_4;
# The file index is used to find the file name in the following table.
:file_names 4;
    "N/A";
    "<interactive>";
    "PC_tb.v";
    "./PC.v";
