 .timescale 0 0;
    .port_info 0 /INPUT 1 "clk";
    .port_info 1 /INPUT 1 "rw_enable";
    .port_info 2 /INPUT 9 "address";
    .port_info 3 /INPUT 16 "data_in";
    .port_info 4 /OUTPUT 16 "data_out";
v0x60e61440d930_0 .net "address", 8 0, v0x60e614433e10_0;  1 drivers
v0x60e614433940_0 .net "clk", 0 0, v0x60e614433ef0_0;  1 drivers
v0x60e614433a00_0 .net "data_in", 15 0, v0x60e614433fc0_0;  1 drivers
v0x60e614433ac0_0 .var "data_out", 15 0;
v0x60e614433ba0 .array "memory", 511 0, 15 0;
v0x60e614433cb0_0 .net "rw_enable", 0 0, v0x60e614434190_0;  1 drivers
E_0x60e61440ef70 .event posedge, v0x60e614433940_0;
    .scope S_0x60e6143e46d0;
T_0 ;
    %wait E_0x60e61440ef70;
    %load/vec4 v0x60e614433cb0_0;
    %flag_set/vec4 8;
    %jmp/0xz  T_0.0, 8;
    %load/vec4 v0x60e61440d930_0;
    %pad/u 11;
    %ix/vec4 4;
    %load/vec4a v0x60e614433ba0, 4;
    %assign/vec4 v0x60e614433ac0_0, 0;
    %jmp T_0.1;
T_0.0 ;
    %load/vec4 v0x60e614433a00_0;
    %load/vec4 v0x60e61440d930_0;
    %pad/u 11;
    %ix/vec4 3;
    %ix/load 4, 0, 0; Constant delay
    %assign/vec4/a/d v0x60e614433ba0, 0, 4;
T_0.1 ;
    %jmp T_0;
    .thread T_0;
    .scope S_0x60e6143e4540;
T_1 ;
    %delay 5, 0;
    %load/vec4 v0x60e614433ef0_0;
    %inv;
    %store/vec4 v0x60e614433ef0_0, 0, 1;
    %jmp T_1;
    .thread T_1;
    .scope S_0x60e6143e4540;
T_2 ;
    %pushi/vec4 0, 0, 1;
    %store/vec4 v0x60e614433ef0_0, 0, 1;
    %pushi/vec4 0, 0, 1;
    %store/vec4 v0x60e614434190_0, 0, 1;
    %pushi/vec4 0, 0, 9;
    %store/vec4 v0x60e614433e10_0, 0, 9;
    %pushi/vec4 0, 0, 16;
    %store/vec4 v0x60e614433fc0_0, 0, 16;
    %pushi/vec4 0, 0, 1;
    %store/vec4 v0x60e614434190_0, 0, 1;
    %pushi/vec4 0, 0, 9;
    %store/vec4 v0x60e614433e10_0, 0, 9;
    %pushi/vec4 4660, 0, 16;
    %store/vec4 v0x60e614433fc0_0, 0, 16;
    %delay 10, 0;
    %pushi/vec4 0, 0, 1;
    %store/vec4 v0x60e614434190_0, 0, 1;
    %pushi/vec4 511, 0, 9;
    %store/vec4 v0x60e614433e10_0, 0, 9;
    %pushi/vec4 17185, 0, 16;
    %store/vec4 v0x60e614433fc0_0, 0, 16;
    %delay 10, 0;
    %pushi/vec4 1, 0, 1;
    %store/vec4 v0x60e614434190_0, 0, 1;
    %pushi/vec4 0, 0, 9;
    %store/vec4 v0x60e614433e10_0, 0, 9;
    %delay 10, 0;
    %vpi_call 2 45 "$display", "Memory[%d] = %h", v0x60e614433e10_0, v0x60e6144340c0_0 {0 0 0};
    %pushi/vec4 1, 0, 1;
    %store/vec4 v0x60e614434190_0, 0, 1;
    %pushi/vec4 511, 0, 9;
    %store/vec4 v0x60e614433e10_0, 0, 9;
