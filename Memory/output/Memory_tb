:vpi_module "/usr/lib/ivl/v2005_math.vpi";
:vpi_module "/usr/lib/ivl/va_math.vpi";
S_0x5afc22402190 .scope module, "Memory_tb" "Memory_tb" 2 1;
 .timescale 0 0;
v0x5afc22451980_0 .var "address", 9 0;
v0x5afc22451a60_0 .var "clk", 0 0;
v0x5afc22451b30_0 .var "data_in", 15 0;
v0x5afc22451c30_0 .net "data_out", 15 0, v0x5afc22451630_0;  1 drivers
v0x5afc22451d00_0 .var "rw_enable", 0 0;
S_0x5afc22402320 .scope module, "dut" "Memory" 2 14, 3 1 0, S_0x5afc22402190;
 .timescale 0 0;
    .port_info 0 /INPUT 1 "clk";
    .port_info 1 /INPUT 1 "rw_enable";
    .port_info 2 /INPUT 10 "address";
    .port_info 3 /INPUT 16 "data_in";
    .port_info 4 /OUTPUT 16 "data_out";
v0x5afc2242b520_0 .net "address", 9 0, v0x5afc22451980_0;  1 drivers
v0x5afc22451480_0 .net "clk", 0 0, v0x5afc22451a60_0;  1 drivers
v0x5afc22451540_0 .net "data_in", 15 0, v0x5afc22451b30_0;  1 drivers
v0x5afc22451630_0 .var "data_out", 15 0;
v0x5afc22451710 .array "memory", 511 0, 15 0;
v0x5afc22451820_0 .net "rw_enable", 0 0, v0x5afc22451d00_0;  1 drivers
E_0x5afc2242cb60 .event posedge, v0x5afc22451480_0;
    .scope S_0x5afc22402320;
T_0 ;
    %wait E_0x5afc2242cb60;
    %load/vec4 v0x5afc22451820_0;
    %flag_set/vec4 8;
    %jmp/0xz  T_0.0, 8;
    %load/vec4 v0x5afc2242b520_0;
    %pad/u 11;
    %ix/vec4 4;
    %load/vec4a v0x5afc22451710, 4;
    %assign/vec4 v0x5afc22451630_0, 0;
    %jmp T_0.1;
T_0.0 ;
    %load/vec4 v0x5afc22451540_0;
    %load/vec4 v0x5afc2242b520_0;
    %pad/u 11;
    %ix/vec4 3;
    %ix/load 4, 0, 0; Constant delay
    %assign/vec4/a/d v0x5afc22451710, 0, 4;
T_0.1 ;
    %jmp T_0;
    .thread T_0;
    .scope S_0x5afc22402190;
T_1 ;
    %delay 5, 0;
    %load/vec4 v0x5afc22451a60_0;
    %inv;
    %store/vec4 v0x5afc22451a60_0, 0, 1;
    %jmp T_1;
    .thread T_1;
    .scope S_0x5afc22402190;
T_2 ;
    %pushi/vec4 0, 0, 1;
    %store/vec4 v0x5afc22451a60_0, 0, 1;
    %pushi/vec4 0, 0, 1;
    %store/vec4 v0x5afc22451d00_0, 0, 1;
    %pushi/vec4 0, 0, 10;
    %store/vec4 v0x5afc22451980_0, 0, 10;
    %pushi/vec4 0, 0, 16;
    %store/vec4 v0x5afc22451b30_0, 0, 16;
    %pushi/vec4 0, 0, 1;
    %store/vec4 v0x5afc22451d00_0, 0, 1;
    %pushi/vec4 0, 0, 10;
    %store/vec4 v0x5afc22451980_0, 0, 10;
    %pushi/vec4 4660, 0, 16;
    %store/vec4 v0x5afc22451b30_0, 0, 16;
    %delay 10, 0;
    %pushi/vec4 0, 0, 1;
    %store/vec4 v0x5afc22451d00_0, 0, 1;
    %pushi/vec4 511, 0, 10;
    %store/vec4 v0x5afc22451980_0, 0, 10;
    %pushi/vec4 17185, 0, 16;
    %store/vec4 v0x5afc22451b30_0, 0, 16;
    %delay 10, 0;
    %pushi/vec4 1, 0, 1;
    %store/vec4 v0x5afc22451d00_0, 0, 1;
    %pushi/vec4 0, 0, 10;
    %store/vec4 v0x5afc22451980_0, 0, 10;
    %delay 10, 0;
    %vpi_call 2 45 "$display", "Memory[%d] = %h", v0x5afc22451980_0, v0x5afc22451c30_0 {0 0 0};
    %pushi/vec4 1, 0, 1;
    %store/vec4 v0x5afc22451d00_0, 0, 1;
    %pushi/vec4 511, 0, 10;
    %store/vec4 v0x5afc22451980_0, 0, 10;
    %delay 10, 0;
    %vpi_call 2 50 "$display", "Memory[%d] = %h", v0x5afc22451980_0, v0x5afc22451c30_0 {0 0 0};
    %vpi_call 2 52 "$finish" {0 0 0};
    %end;
    .thread T_2;
